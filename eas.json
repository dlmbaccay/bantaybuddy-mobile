{
  "cli": {
    "version": ">= 14.5.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "env": {
          "GOOGLE_SERVICES_JSON": "@GOOGLE_SERVICES_JSON"
        }
      },
      "ios": {
        "GOOGLE_SERVICES_INFO_PLIST": "@GOOGLE_SERVICES_INFO_PLIST"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "env": {
          "GOOGLE_SERVICES_JSON": "@GOOGLE_SERVICES_JSON"
        }
      },
      "ios": {
        "GOOGLE_SERVICES_INFO_PLIST": "@GOOGLE_SERVICES_INFO_PLIST"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk",
        "env": {
          "GOOGLE_SERVICES_JSON": "@GOOGLE_SERVICES_JSON"
        }
      },
      "ios": {
        "GOOGLE_SERVICES_INFO_PLIST": "@GOOGLE_SERVICES_INFO_PLIST"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
